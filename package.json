{
  "name": "ecombe",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn --cwd client start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "execMap": {
      "ts": "ts-node"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.528.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongoose": "^5.7.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "razorpay": "^2.0.5",
    "socket.io": "^2.2.0",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2"
  }
}
